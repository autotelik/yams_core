$("#messages_container").html("<%= j render(partial: 'yams_core/shared/messages') %>");

<% if(@album.errors.blank?) %>
  $('#render-new-album-form').html("<%= j render(partial: 'yams_core/album/albums/form', locals: { album: YamsCore::Album.new(user: current_user) } ) %>");

  $('#target-for-album-create-insertion-point').after("<li class=\"list-group-item\"><%= j render(partial: 'yams_core/album/album_with_dropzone', locals: { album_presenter: @album } ) %></li>");
<% end %>
